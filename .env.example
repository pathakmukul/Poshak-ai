# Copy this file to .env and add your actual API tokens

# Replicate API Configuration
# Get your API token from: https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=r8_YOUR_ACTUAL_TOKEN_HERE

# FAL API Configuration
# Get your API key from: https://fal.ai
FAL_KEY=YOUR_FAL_API_KEY_HERE

# Google Gemini API Configuration
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Segformer Model Settings
# Set to 'true' to use Hugging Face API instead of local model
USE_HF_API=false
# Optional: HF API token for private models or higher rate limits
HUGGINGFACE_API_TOKEN=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

# Firebase Configuration
# Path to your Firebase service account key JSON file
# Download from: https://console.firebase.google.com/project/poshakai/settings/serviceaccounts/adminsdk
GOOGLE_APPLICATION_CREDENTIALS=./serviceAccountKey.json
# Your Firebase Storage bucket name
FIREBASE_STORAGE_BUCKET=poshakai.appspot.com
